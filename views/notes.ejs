<h1 class="text-center mt-5 mb-5" style="color: blue">Download Notes by CodeWithRahul</h1>
<div class="main_container_notes">
    <%for(let i = 0; i < notes.length; i++){%>
    <div class="container_notes">
        <div class="img_container" style="height: 10rem;width: 10rem;">
            <div class="" style="width: 80%;height: 80%; border-radius: 50%;overflow: hidden;">
                <img src="<%=notes[i].image%>" alt="" style="width: 100%;height: 100%;">
            </div>
        </div>
        <div class="info_container" style="margin:1rem">
            <h3><%=notes[i].title%></h3>
            <p>Download Notes Here</p>
        </div>
        <div class="btn_container">
           <a  href="<%=notes[i].link%>" download><button class="btn btn-primary rounded">PDF Notes</button></a> 
        </div>
    </div>
    <%}%>
</div>
