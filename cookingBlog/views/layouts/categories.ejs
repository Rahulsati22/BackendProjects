<h1 class="my-3 mx-3 text-success" style="font-weight: 700;">Categories</h1>
<div class="row mx-2 my-5" >
  <%for (let i = 0; i < categoryy.length; i++){%>
  <a href="/categoryCountry/<%=categoryy[i].name%>"
    class="col-lg-2 rounded"
    style="height: 14rem; overflow: hidden; cursor: pointer;text-decoration: none;"
    id="categories"
  >
    <img
      src="<%=categoryy[i].image%>"
      alt=""
      class="cate"
    />
    <p style="text-align: center" class="my-3 text-success"><%=categoryy[i].name%></p>
  </a>
  <%}%>
</div>
